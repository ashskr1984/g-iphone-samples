.contentSearchContainer {

}

.contentSearch {
	border: solid 1px #bfd6ff;
	padding: 10px 20px 10px 20px;
	margin-bottom: 15px;
	margin-right: 8px;
	text-align: center;
	font-weight: bold;
	font-size: 13px;
	float: left;
