}

function hide_removing_rights_alert(form_element) {
   var rightsWarning = $('removing_rights_warning');
   var cellbgcolor = form_element.parentNode.parentNode.bgColor;
   if (cellbgcolor == null) {
       cellbgcolor = "#ffffff";
